<div class="container py-4">
    <div class="container px-4 py-2 my-5 text-center">

        <h1 class="display-5 fw-bold text-body-emphasis">CLUBS</h1>

    </div>

    <div class="container px-4">
        <div class="container py-5 ">
            <div class="row row-cols-5 row-cols-lg-4 g-2 g-lg-4">
                <% if(typeof clubs!=="undefined" && clubs.length>0) { %>
                    <% clubs.forEach(function(club,index){%>
                        <a href="/clubs/<%=club.name %>"
                            class="category__link text-decoration-none col text-center text-dark">
                            <div class="category__img category__img--large shadow">
                                <img src="/images/<%=club.image%>" alt="<%=club.name%>" loading="lazy">
                            </div>
                            <br>
                                <div class="pt-1 text-bg-dark">
                                    <%=club.name%>
                                </div>
                            
                        </a>
                        <%})%>
                            <%} else {%>
                                <p>No items found</p>

                                <%}%>

            </div>
        </div>
    </div>
    <div class="container my-4">
        <h2 class="pb-2 border-bottom"></h2>
    </div>


</div>

